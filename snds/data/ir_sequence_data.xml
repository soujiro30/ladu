<odoo>
    <data>
        <record id="seq_school_needs" model="ir.sequence">
            <field name="name">Schools' Needs</field>
            <field name="code">school.needs</field>
            <field name="prefix">SNDS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_school_product" model="ir.sequence">
            <field name="name">Product/Item</field>
            <field name="code">school.product</field>
            <field name="prefix">SP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_stakeholder_project" model="ir.sequence">
            <field name="name">Stakeholder</field>
            <field name="code">stakeholder.project</field>
            <field name="prefix">A</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>